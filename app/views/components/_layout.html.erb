<% content_for :head do %>
  <%= stylesheet_link_tag "application", integrity: true, crossorigin: 'anonymous' %>
  <%= javascript_include_tag "application", integrity: true, crossorigin: 'anonymous' %>
<% end %>

<% content_for :content do %>
  <div id="wrapper" class="<%= wrapper_class %>">
    <%= render_phase_label @content_item, content_for(:phase_message) %>
    <% if show_step_nav?%>
      <%= render 'shared/step_nav_header', step_nav_content: current_step_nav %>
    <% else %>
      <% if !content_for(:no_breadcrumbs) %>
        <%= render 'shared/breadcrumbs' %>
      <% end %>
    <% end %>
    <main role="main" id="content" class="<%= local_assigns[:page_class] %>" lang="<%= I18n.locale %>">
      <%= yield %>
    </main>
  </div>
<% end %>

<%= render file: 'layouts/govuk_template' %>
